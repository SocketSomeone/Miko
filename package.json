{
	"name": "miko",
	"version": "0.0.8",
	"description": "",
	"private": true,
	"main": "./bin/root.js",
	"scripts": {
		"prebuild": "rimraf bin",
		"build": "tsc",
		"start": "cross-env NODE_ENV=dev node scripts/dev-run.js",
		"release": "yarn build && yarn format && standard-version",
		"typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
		"migrate": "yarn typeorm migration:generate -n MigrateEnitity && yarn typeorm migration:run",
		"format": "prettier --write \"src/**/*.ts\"",
		"nxv": "node ./scripts/npm-exact-versions.js",
		"gc": "node ./scripts/generate-commands.js",
		"i18n": "node scripts/check-i18n.js",
		"ssh": "ssh root@185.185.68.44",
		"deploy": "yarn build && pscp -r ./bin root@185.185.68.44:/root/Miko",
		"deployLang": "pscp -r ./i18n root@185.185.68.44:/root/Miko"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn i18n && yarn nxv && yarn pretty-quick --staged"
		}
	},
	"standard-version": {
		"skip": {
			"changelog": true
		}
	},
