{
	"name": "miko",
	"version": "0.0.3",
	"description": "",
	"main": "./bin/root.js",
	"scripts": {
		"build": "tsc",
		"start": "node scripts/dev-run.js",
		"release": "yarn build && yarn format && standard-version",
		"typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
		"migrate": "yarn typeorm migration:generate -n MigrateEnitity && yarn typeorm migration:run",
		"format": "prettier --write \"src/**/*.ts\"",
		"nxv": "node ./scripts/npm-exact-versions.js",
		"i18n": "node scripts/check-i18n.js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn i18n && yarn nxv && yarn pretty-quick --staged"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SocketSomeone/Miko.git"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/chalk": "2.2.0",
		"@types/glob": "7.1.3",
		"@types/i18n": "0.8.6",
		"@types/moment": "2.13.0",
		"@types/node": "14.0.14",
		"eslint": "7.5.0",
		"husky": "4.2.5",
		"prettier": "2.0.5",
		"pretty-quick": "2.0.1",
		"typescript": "3.9.5"
	},
	"dependencies": {
		"@sentry/cli": "1.55.0",
		"@sentry/node": "5.20.0",
		"@types/amqplib": "^0.5.13",
		"amqplib": "^0.6.0",
		"chalk": "4.1.0",
		"dotenv": "8.2.0",
		"eris": "0.13.3",
		"glob": "7.1.6",
		"i18n": "0.10.0",
		"moment": "2.27.0",
		"moment-duration": "0.0.6",
		"pg": "8.2.1",
		"reflect-metadata": "0.1.13",
		"typeorm": "0.2.25"
	},
	"engines": {
		"node": ">=10.16.3 <12.16.1"
	}
}
