{
    "compilerOptions": {
        "target": "ESNext",
		"module": "CommonJS",
		"moduleResolution": "Node",
		"emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "esModuleInterop": true,
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true,
		"strict": true,
        "allowJs": true,
        "importHelpers": true,
        "noEmitHelpers": true,
		"forceConsistentCasingInFileNames": true
    }
}